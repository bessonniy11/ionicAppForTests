{
  "name": "ionicApp",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "run web dev": "ng run web:serve",
    "run dev": "ng serve --port 9900",
    "run web stage": "ng run web:serve:stage",
    "run web staging": "ng run web:serve:staging",
    "run web demo": "ng run web:serve:demo",
    "run web production": "ng run web:serve:production",
    "run mobile dev": "ng run mobile:serve --port 9900",
    "run mobile stage": "ng run mobile:serve:stage --port 9000",
    "run mobile staging": "ng run mobile:serve:staging --port 9000",
    "run mobile demo": "ng run mobile:serve:demo --port 9000",
    "run mobile production": "ng run mobile:serve:production --port 9000",
    "run ssr dev": "ng run web:serve-ssr --port 4000",
    "run ssr production": "ng run web:serve-ssr:production --port 4000",
    "build ssr dev": "ng run web:build && ng run web:server",
    "build ssr production": "ng run web:build:production && ng run web:server:production",
    "run:ssr": "node dist/app/server/main.js",
    "dev:ssr": "ng run web:serve-ssr",
    "prerender": "ng run web:prerender",
    "build mobile dev": "ionic cordova build android --configuration=development",
    "start dev android": "ionic cordova run android -l --device",
    "build dev android apk": "ionic cordova build android --prod --release --configuration=development --no-resources --buildConfig=build_apk.json",
    "build dev android bundle": "ionic cordova build android --prod --release --configuration=development --no-resources --buildConfig=build_bundle.json",
    "build staging android apk": "ionic cordova build android --prod --release --configuration=staging --no-resources --buildConfig=build_apk.json",
    "build staging android bundle": "ionic cordova build android --prod --release --configuration=staging --no-resources --buildConfig=build_bundle.json"
  },
  "private": true,
  "dependencies": {
